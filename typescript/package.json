{
  "name": "@carbonapi/typescript-sdk",
  "version": "3.0.0",
  "description": "TypeScript SDK for CarbonAPI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "generate-types": "openapi-typescript openapi.json --output src/types/api.ts"
  },
  "keywords": [
    "carbonapi",
    "sdk",
    "carbon",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/carbon-api/sdks"
  },
  "bugs": {
    "url": "https://github.com/carbon-api/sdks/issues"
  },
  "homepage": "https://github.com/carbon-api/sdks#readme",
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@eslint/js": "^9.39.1",
    "@types/express": "^4.17.25",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.24",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "express": "^4.21.2",
    "jest": "^29.7.0",
    "openapi-typescript": "^7.10.1",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  },
  "dependencies": {
    "exponential-backoff": "^3.1.3",
    "openapi-fetch": "^0.10.6",
    "svix": "^1.81.0"
  }
}
